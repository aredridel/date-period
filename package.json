{
  "name": "date-period",
  "version": "2.0.1",
  "description": "Time period iterator",
  "main": "dist/period.js",
  "scripts": {
    "lint": "eslint src test",
    "coverage": "TZ=Europe/Brussels istanbul cover _mocha test -- --require babel-register -R spec && opn coverage/lcov-report/index.html",
    "coveralls": "TZ=Europe/Brussels istanbul cover _mocha --report lcovonly test -- --require babel-register -R spec && cat ./coverage/lcov.info | coveralls",
    "build": "babel --source-maps -d dist/ src/",
    "mocha": "TZ=Europe/Brussels mocha --compilers js:babel-register test",
    "test": "npm run lint && npm run build && npm run mocha",
    "clean": "rm -rf dist && rm -rf coverage",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run test",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smhg/date-period.git"
  },
  "keywords": [
    "time",
    "period",
    "iterator",
    "generator",
    "iso",
    "8601",
    "iso8601",
    "iso 8601",
    "date"
  ],
  "author": "Sam Hauglustaine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smhg/date-period/issues"
  },
  "homepage": "https://github.com/smhg/date-period#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-register": "6.18.0",
    "coveralls": "2.11.15",
    "eslint": "3.12.2",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mocha-lcov-reporter": "1.2.0",
    "opn-cli": "3.1.0"
  },
  "dependencies": {
    "date-duration": "1.0.0"
  },
  "peerDependencies": {
    "babel-polyfill": "^6.6.0"
  }
}
