{
  "name": "date-period",
  "version": "1.0.0",
  "description": "Time period iterator",
  "main": "dist/period.js",
  "scripts": {
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel/register test",
    "coverage": "istanbul cover _mocha test -- --require babel/register -R spec && opn coverage/lcov-report/index.html",
    "coveralls": "istanbul cover _mocha --report lcovonly test -- --require babel/register -R spec && cat ./coverage/lcov.info | coveralls",
    "preversion": "npm run lint && npm test",
    "postversion": "git push && git push --tags",
    "build": "npm run lint && npm test && babel src --optional runtime --source-maps -d dist",
    "clean": "rm -rf dist && rm -rf coverage",
    "prepublish": "npm run build",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smhg/date-period.git"
  },
  "keywords": [
    "time",
    "period",
    "iterator",
    "generator",
    "iso",
    "8601",
    "iso8601",
    "iso 8601",
    "date"
  ],
  "author": "Sam Hauglustaine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smhg/date-period/issues"
  },
  "homepage": "https://github.com/smhg/date-period#readme",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "coveralls": "^2.11.3",
    "eslint": "^1.1.0",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "opn-cli": "^1.0.0"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "date-duration": "^0.1.0"
  }
}
